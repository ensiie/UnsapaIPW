<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}UnsapaIPW{% endblock %}</title>
        <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />
        {% stylesheets '@UnsapaIPWBundle/Resources/public/css/application.css' %}
          <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

        <link type="text/css" href="{{ asset("bundles/unsapaipw/css/ui-lightness/jquery-ui-1.8.20.custom.css") }}" rel="Stylesheet" />  
        <script type="text/javascript" src=" {{ asset("bundles/unsapaipw/js/jquery-1.7.1.min.js") }} "></script>
        <script type="text/javascript" src="{{ asset("bundles/unsapaipw/js/jquery-ui-1.8.20.custom.min.js") }}"></script>
        {% block header_js %}{% endblock %}

    </head>
    <body>
        {% block menu %}
            <nav>
                <a alt="Mes examens" href="{{ path('exams') }}">
                  <img src="{{ asset('bundles/unsapaipw/images/documents.png') }}" width="75px" height="75px" alt="Mes examens" />
                </a>
            </nav>
        {% endblock %}

        <div id="content">
        {% block content %}
        {% endblock %}
        </div>

        <div class="fos_user_content">
            {% block fos_user_content %}
            {% endblock %}
        </div>
        <footer>
            {% block footer_js %}
            {% endblock %}
        </footer>
    </body>
</html>
